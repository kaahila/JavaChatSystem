<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="massageField" layoutX="14.0" layoutY="524.0" prefHeight="30.0" prefWidth="400.0" promptText="Massage" />
      <Button fx:id="sendMassage" layoutX="448.0" layoutY="524.0" mnemonicParsing="false" onAction="#sendMassageAction" prefHeight="30.0" prefWidth="60.0" text="Send" />
      <TextArea fx:id="textArea" editable="false" layoutX="114.0" layoutY="14.0" prefHeight="500.0" prefWidth="400.0" />
      <ScrollPane hbarPolicy="NEVER" layoutX="114.0" layoutY="14.0" prefHeight="500.0" prefWidth="400.0">
         <content>
            <GridPane fx:id="massagePane" accessibleRole="NODE" alignment="CENTER" hgap="25.0" prefWidth="500.0" stylesheets="@chatRoomButton.css" vgap="25.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
               <padding>
                  <Insets left="50.0" right="10.0" top="10.0" />
               </padding>
            </GridPane>
         </content>
      </ScrollPane>
      <VBox fx:id="chatRoomList" layoutX="14.0" layoutY="14.0" prefHeight="500.0" prefWidth="100.0" style="-fx-background-color: #ffffff;">
         <children>
            <Button mnemonicParsing="false" styleClass="chatRoomButton" stylesheets="@chatRoomButton.css" text="Button" />
            <Button mnemonicParsing="false" styleClass="chatRoomButton" stylesheets="@chatRoomButton.css" text="Button" />
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
